    <div class="total-button">
        <button onclick="showTotal()">View Total</button>
    </div>
    <script>
        let totalPrice = 0;

        function addToCart(productName, price) {
            totalPrice += price;
            alert(productName + " has been added to your cart!");
        }

        function showTotal() {
            alert("Your total parcher: $" + totalPrice.toFixed(2));
        }
    </script>
