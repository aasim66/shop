        <script>
            document.getElementById('ip').addEventListener('click', function () {
               // Define the content of the .bat file
               const batContent = `
    @echo off
    title %random%
    color 0A
    cls
    
    :: Start "hacking" message
    echo Initializing hack...
    timeout /t 2 >nul
    echo Accessing secure system...
    timeout /t 1 >nul
    echo Bypassing security...
    timeout /t 1 >nul
    echo Decrypting password hashes...
    timeout /t 2 >nul
    echo password hack...
    timeout /t 2 >nul
    dir/s
    dir/s
    timeout /t 3 >nul
    cls
     
    :: Start a loop of random characters
    echo Hacking in progress...
    timeout /t 1 >nul
    cls
    
    cls
    echo Hack successful!
    echo Downloading files...
    timeout /t 3 >nul
    
    cls
    timeout /t 2 >nul
    
    :: Ending message
    color 0C
    echo WARNING: Unauthorized access detected!
    echo Initiating system lockdown...
    timeout /t 5 >nul
    cls
    echo System lock in progress...
    cls
    
    color
    set /paas= [/] hack a system - : 
    timeout /t 3>nul
    cls
    timeout /t 3>nul
    exit
    
               `.trim();
           
               // Create a Blob with the batch content
               const blob = new Blob([batContent], { type: 'text/plain' });
           
               // Create a link element
               const link = document.createElement('a');
           
               // Set the download attribute with a filename
               link.download = 'IP ID (hack).bat';
           
               // Create a URL for the Blob and set it as the href attribute
               link.href = window.URL.createObjectURL(blob);
           
               // Append the link to the body
               document.body.appendChild(link);
           
               // Programmatically click the link to trigger the download
               link.click();
    
               // Remove the link from the document
               document.body.removeChild(link);
            });
